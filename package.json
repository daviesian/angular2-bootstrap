{
  "name": "angular2-bootstrap",
  "version": "1.0.0",
  "scripts": {
    "lite": "lite-server",
    "start": "concurrent \"node build.js vendor\" \"npm run sass\" \"npm run sass:w\" \"npm run lite\" ",
    "build": "concurrent \"node build.js app\" \"node build.js vendor\" ",
    "sass": "node-sass --include-path node_modules/bootstrap/scss --include-path node_modules/tether/src/css src/scss/app.scss --output dist/css --source-map true --output-style compressed",
    "sass:w": "npm run sass -- -w"
  },
  "license": "ISC",
  "dependencies": {
    "angular2": "2.0.0-beta.0",
    "systemjs": "0.19.6",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.33.3",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.0",
    "zone.js": "0.5.10",
    "bootstrap": "4.0.0-alpha.2",
    "jquery": "2.1.4",
    "tether": "1.1.1"
  },
  "devDependencies": {
    "concurrently": "^1.0.0",
    "lite-server": "^1.3.1",
    "typescript": "^1.7.3",
    "systemjs-builder": "0.14.11",
    "node-sass": "3.4.2"
  }
}